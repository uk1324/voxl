class Range {
	$init(n) {
		$.current = 0;
		$.max = n;
		ret $;
	}

	$iter() {
		ret $;
	}

	$next() {
		if $.current == $.max {
			throw StopIteration();
		}
		result : $.current;
		$.current += 1;
		ret result;
	}
}


for i in Range(3) {
	for j in Range(3) {
		put("(" ++ i ++ "," ++ j ++ ")");
	}
}