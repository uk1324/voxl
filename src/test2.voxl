fn sum(f, n) {
	let sum = 0;
	let i = 0;
	loop {
		sum = sum + f(i);
		if i == n {
			break;
		}
		i = i + 1;
	}
	ret sum;
}

fn fact(x) {
	if (x == 1) || (x == 0) {
		ret 1;
	}
	ret fact(x - 1) * x;
}

let i = 0;
loop {
	print(fact(i) ++ "\n");
	if i == 10 {
		break;
	}
	i = i + 1;
}

